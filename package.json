  },
  "author": "pancomputationalist@mailbox.org",
  "license": "ISC",
  "peerDependencies": {
    "graphql": "^14.0.2",
    "graphql-code-generator": "^0.14.1",
    "react": "^16.7.0-alpha.2"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.7.7",
    "apollo-client": "^2.4.7",
    "jest": "^23.6.0",
    "react-apollo": "^2.3.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.6"
  }
}
